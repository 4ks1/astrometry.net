
RESAMPLE_DIR := $(BASEDIR)/base
RESAMPLE_LIB_FILE := $(RESAMPLE_DIR)/libresample.a

RESAMPLE_CFLAGS :=
RESAMPLE_INC := -I$(INCLUDE_BASE_DIR)/resample
RESAMPLE_LIB := 
RESAMPLE_SLIB := $(RESAMPLE_LIB_FILE)

$(RESAMPLE_LIB_FILE):
	$(MAKE) -C $(RESAMPLE_DIR) libresample.a

include $(MAKEFILES_DIR)/makefile.anbase

RESAMPLE_INC += $(ANBASE_INC)
